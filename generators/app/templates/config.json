{
  "app": {
    "runMode": "debug",
    "port": "<%= serverPort %>",
    "key": "<%= appSecretKey %>",
    "cipher": "AES-256-CBC",
    "readTimeout": 60,
    "writeTimeout": 60,
    "SSL": false,
    "jwtSecretKey": "<%= jwtSecretKey %>",
    "pageSize": 10
  },
  "log": {
    "default": "stack",
    "level": {
      "ROOT": "DEBUG"
    },
    "callerDepth": 2,
    "drivers": {
      "stack": {
        "channels": [
          "file", "system"
        ]
      },
      "file": {
        "runtimeRootPath": "runtime/",
        "savePath": "logs/",
        "saveName": "log",
        "ext": "log",
        "timeFormat": "20060102"
      },
      "slack": {
        "url": "LOG_SLACK_WEBHOOK_URL",
        "username": "Laravel Log",
        "emoji": ":boom:"
      },
      "system": {},
      "database": {
        "table": "logs"
      },
      "null": {}
    }
  },
  "db": {
    "default": "sqlite3",
    "drivers": {
      "sqlite3": {
        "path": "./database/<%= appName %>.db"
      },
      "pgsql": {
        "host": "localhost",
        "port": 5432,
        "database": "<%= appName %>",
        "username": "admin",
        "password": "admin"
      }
    }
  },
  "cache": {
    "default": "memcached",
    "drivers": {
      "memcached": {
        "defaultExpiration": 300,
        "purgeDuration": 600
      },
      "null": {},
      "redis": {
        "host": "localhost",
        "port": 6379,
        "password": "",
        "maxIdle": 30,
        "maxActive": 30,
        "idleTimeout": -1
      }
    }
  },
  "mail": {
    "default": "smtp",
    "mailers": {
      "smtp": {
        "host": "smtp.mailgun.org",
        "port": 587,
        "username": "",
        "password": ""
      },
      "mailgun": {},
      "postmark": {},
      "sendgrid": {},
      "ses": {}
    }
  },
  "sms": {
    "default": "vonage",
    "drivers": {
      "vonage": {
        "apiKey": "",
        "apiSecret": ""
      }
    }
  },
  "hash": {
    "default": "bcrypt",
    "drivers": {
      "bcrypt": {}
    }
  },
  "fcm": {
    "apiKey": "AIza...",
    "parentId": "abc"
  },
  "queue": {
    "default": "null",
    "drivers": {
      "RabbitMQ": {
        "host": "localhost",
        "port": 5672,
        "username": "",
        "password": ""
      }
    }
  }
}
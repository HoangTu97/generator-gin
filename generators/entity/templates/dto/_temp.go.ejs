package dto

import (
  "time"
  "gorm.io/gorm"
)

// <%= entityClass %>DTO godoc
type <%= entityClass %>DTO struct {
  ID uint
  CreatedAt time.Time
  UpdatedAt time.Time
  DeletedAt gorm.DeletedAt
<% fields.forEach(function(field){ %>
  <%= field.name %> <%= field.type %> <% if (field.jsonConfig !== '') {%>`<%- field.jsonConfig %>`<%}%><% }); %>
}

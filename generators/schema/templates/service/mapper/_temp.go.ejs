package mapper

import (
  "<%= appName %>/dto"
  "<%= appName %>/models"
)

type <%= entityClass %> interface {
  ToDTO(entity models.<%= entityClass %>) dto.<%= entityClass %>DTO
  ToEntity(dto dto.<%= entityClass %>DTO) models.<%= entityClass %>
  ToDTOS(entityList []models.<%= entityClass %>) []dto.<%= entityClass %>DTO
  ToEntities(dtoList []dto.<%= entityClass %>DTO) []models.<%= entityClass %>
}
